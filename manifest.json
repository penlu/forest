{
  "manifest_version": 2,

  "name": "Forest",
  "description": "A tree-based visualization/classification of internet browsing history.",
  "version": "1.0",

  "icons": {
    "512": "forest.png"
  },

  "permissions": [
    "webNavigation",
    "storage"
  ],

  "browser_action": {
    "default_icon": "forest.png"
    //"default_popup": "popup.html"
  },

  "background": {
    "page": "background.html"
  }
}